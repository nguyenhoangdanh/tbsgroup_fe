{
  "name": "tbsgroup_management",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:analyze": "ANALYZE=true next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "start:production": "NODE_ENV=production next start",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint .",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=true eslint . --fix",
    "type-check": "node tscheck.js",
    "type-check:watch": "node tscheck.js --watch",
    "check": "node tscheck.js --lint",
    "check:verbose": "node tscheck.js --lint --verbose",
    "fix:imports": "node ./scripts/organize-imports.js",
    "fix:security": "node ./scripts/fix-security-service.js",
    "revert": "node ./scripts/revert-imports.js && node ./scripts/uncomment-code.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:coverage": "jest --coverage",
    "security:audit": "npm audit --audit-level high",
    "security:check": "npm audit && npm run type-check",
    "performance:lighthouse": "lhci autorun",
    "performance:bundle": "npm run build:analyze",
    "deploy:staging": "npm run build && npm run test:ci && echo 'Deploy to staging'",
    "deploy:production": "npm run security:check && npm run build:production && npm run test:ci && echo 'Deploy to production'",
    "env:validate": "node scripts/validate-env.js",
    "env:copy": "cp .env.example .env.local",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json && npm install",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "monitor:start": "npm run dev & npm run health:check",
    "postinstall": "npm run env:validate || echo 'Environment validation skipped'"
  },
